                "gesamtrenditeIndex": {
                    "type": "double"
                },
                "gesamtrenditeKapital": {
                    "type": "double"
                },
                "veraenderungStartkapital": {
                    "type": "double"
                },
                "veraenderungStartkapitalGeld": {
                    "type": "double"
                },
                "totalRenteeinzahlungen": {
