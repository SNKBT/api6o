                    "type": "string",
                    "format": "date"
                },
                "adjClose": {
                    "type": "double"
                },
                "anteile": {
                    "type": "double"
                },
                "wert": {
                    "type": "double"
                },
                "buySMA": {
                    "type": "double"
                },
                "sellSMA": {
                    "type": "double"
                }
            },
            "required": [
